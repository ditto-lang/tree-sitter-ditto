{
  "name": "tree-sitter-ditto",
  "repository": {
    "type": "git",
    "url": "https://github.com/ditto-lang/tree-sitter-ditto.git"
  },
  "version": "0.0.1",
  "description": "Ditto grammar for the tree-sitter parsing library",
  "author": "Jordan Mackie <jordan@nous.co> (https://github.com/jmackie)",
  "main": "bindings/node",
  "keywords": [
    "ditto",
    "ditto-lang",
    "tree-sitter",
    "parser",
    "parsing",
    "lexer",
    "incremental"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "nan": "2.15.0"
  },
  "devDependencies": {
    "chokidar-cli": "3.0.0",
    "prettier": "2.6.0",
    "tree-sitter-cli": "0.20.6"
  },
  "scripts": {
    "pretest": "tree-sitter generate",
    "test": "tree-sitter test",
    "dev": "chokidar \"test/**/*\" \"queries/*\" grammar.js --initial -c \"npm run test\""
  },
  "tree-sitter": [
    {
      "scope": "source.ditto",
      "file-types": [
        "ditto"
      ]
    }
  ],
  "prettier": {
    "arrowParens": "avoid"
  }
}
